{{! Content of sent-message notifications }}
<div class="above_composebox_banner {{banner_type}} {{classname}}">
    <p>
        {{banner_text}}
        {{#if link_text}} <a class="above_composebox_banner_action_link" href="{{above_composebox_narrow_url}}" data-message-id="{{link_msg_id}}">{{link_text}}</a>{{/if}}
    </p>
    <a role="button" class="zulip-icon zulip-icon-close above_composebox_banner_close_button"></a>
</div>
