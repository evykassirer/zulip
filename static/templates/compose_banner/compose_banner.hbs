<div class="compose_banner {{banner_type}} {{classname}}" data-stream-id="{{stream_id}}" data-topic-name="{{topic_name}}">
    {{#if banner_text}}
    <p>{{banner_text}}</p>
    {{else}}
    <div>{{> @partial-block}}</div>
    {{/if}}
    {{#if button_text}}
    <button class="compose_banner_action_button" >{{button_text}}</button>
    {{/if}}
    <a role="button" class="zulip-icon zulip-icon-close compose_banner_close_button"></a>
</div>
